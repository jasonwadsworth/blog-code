{
    "name": "delayed-event-processing",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "test": "yarn install --frozen-lockfile ./node_modules/.bin/tsc && jest",
        "build": "rm -rf node_modules && yarn install --frozen-lockfile && ./node_modules/.bin/tsc && rm -rf node_modules && yarn install --frozen-lockfile --production && cp deploy.yaml ./dist/deploy.yaml && rsync -aq --exclude='.*' ./node_modules ./dist && yarn install",
        "package": "yarn build && aws cloudformation package --template-file ./dist/deploy.yaml --output-template-file ./dist/template.yaml",
        "deploy": "aws cloudformation deploy --stack-name delayed-event-processing --template-file ./dist/template.yaml --capabilities CAPABILITY_IAM"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.64",
        "@types/chunk": "^0.0.0",
        "@types/jest": "^26.0.15",
        "@types/jsonpath": "^0.2.0",
        "@types/node": "^14.14.6",
        "@types/uuid": "^8.3.0",
        "aws-sdk": "^2.787.0",
        "get-port": "^5.1.1",
        "jest": "^26.6.3",
        "mockdate": "^3.0.2",
        "node-docker-api": "^1.1.22",
        "ts-jest": "^26.4.4",
        "typescript": "^4.0.5"
    },
    "dependencies": {
        "chunk": "^0.0.3",
        "jsonpath": "^1.0.2",
        "moment": "^2.29.1",
        "nanoid": "^3.1.16"
    },
    "engine": {
        "node": "=12.0"
    }
}
